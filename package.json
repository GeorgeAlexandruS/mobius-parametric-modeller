{
  "name": "mobius-core",
  "version": "0.0.1",
  "license": "MIT",
  "config": {
    "dev_url": "https://design-automation.github.io/mobius-core/",
    "prod_url": "https://design-automation.github.io/mobius-core/"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "build": "ng build",
    "build-dev": "ng build --base-href=%npm_package_config_dev_url% && rm -r ./docs && cp -r ./dist ./docs && rm -r ./dist",
    "build-prod": "ng build --base-href=%npm_package_config_prod_url% && rm -r ./docs && cp -r ./dist ./docs && rm -r ./dist",
    "build-server": "ng build --base-href='./'",
    "build-deploy": "ng build --base-href \"https://design-automation.github.io/mobius-core/\" && ngh",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "typedoc": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --out docs/typedoc src/app/",
    "moduleDocs": "typedoc --mode modules -module commonjs --exclude **/*.spec.ts --json src/assets/typedoc-json/doc.json src/app/core/modules"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^6.1.7",
    "@angular/forms": "^6.1.7",
    "@angular/material": "^5.0.0-rc.2",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@types/circular-json": "^0.4.0",
    "angular-split": "^1.0.4",
    "circular-json": "^0.4.0",
    "core-js": "^2.5.5",
    "hammerjs": "^2.0.8",
    "jshint": "^2.9.6",
    "rxjs": "^6.3.2",
    "rxjs-compat": "^6.3.2",
    "typescript-compiler": "^1.4.1-2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.7",
    "@angular/cli": "^7.0.6",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^6.1.7",
    "@types/jasmine": "^2.8.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^9.6.39",
    "@types/three": "^0.84.28",
    "codelyzer": "^4.3.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "node-sass": "^4.10.0",
    "protractor": "^5.3.1",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "typescript": "^3.1.6"
  }
}
