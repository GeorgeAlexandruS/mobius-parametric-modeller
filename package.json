    "@angular/forms": "^6.1.7",
    "@angular/material": "^5.0.0-rc.2",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@types/circular-json": "^0.4.0",
    "@types/three": "^0.92.24",
    "angular-split": "^1.0.0-rc.3",
